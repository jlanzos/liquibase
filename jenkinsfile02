pipeline {
    agent any
    stages {
        stage('Version') {
            steps {
                script {
                         bat "docker run --rm   liquibase/liquibase --show-banner=False --version"
                       }
                 }
           }
       
        stage('Status') {
            steps {
                script {
                         bat 'docker run --rm   liquibase/liquibase --show-banner=False  --url="jdbc:sqlserver://192.168.1.139:1433;databaseName=Vueling_SAP;queryTimeout=5;trustServerCertificate=true;integratedsecurity=False;defaultSchemaName=dbo" --driver=com.microsoft.sqlserver.jdbc.SQLServerDriver --classpath=internal/lib/mssql-jdbc.jar --username=sa --password=lan000 --changelog-file=mychangelog002.sql status --verbose'

                       }
                 }
           }
         }		 
   	
}	