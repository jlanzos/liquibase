pipeline {
    agent any
    stages {
        stage('Version') {
            steps {
                script {
                         bat "docker run --rm   liquibase/liquibase --show-banner=False --version"
                       }
                 }
           }
         }
        stage('Status') {
            steps {
                script {
                         bat '"c:\\Program Files\\liquibase\\liquibase" --show-banner=False  --url="jurl:jdbc:sqlserver://192.168.1.139:1433;databaseName=Vueling_SAP;queryTimeout=5;trustServerCertificate=true;integratedsecurity=False;defaultSchemaName=dbo"  status --verbose'

                       }
                 }
           }
         }		 
    }	
}	